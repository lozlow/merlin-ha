    "amd64",
    "aarch64",
    "armv7"
  ],
  "audio": true,
  "video": false,
  "gpio": false,
  "kernel_modules": false,
  "devicetree": false,
  "docker_api": false,
  "privileged": [],
  "full_access": false,
  "apparmor": true,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": false,
  "host_network": false,
  "host_pid": false,
  "host_ipc": false,
  "host_dbus": false,
  "devices": [],
  "udev": false,
  "tmpfs": false,
  "environment": {},
  "labels": {
    "io.hass.name": "BirdNET-Go",
