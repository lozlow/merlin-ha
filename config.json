    "latitude": "float?",
    "longitude": "float?",
    "database_type": "list(sqlite|mysql)",
    "database_path": "str",
    "clip_path": "str",
    "log_path": "str",
    "web_enabled": "bool",
    "web_port": "port",
    "realtime_enabled": "bool",
    "privacy_filter": "bool",
    "night_filter": "bool",
    "species_list_enabled": "bool",
    "range_filter_enabled": "bool",
    "prometheus_enabled": "bool",
    "prometheus_port": "port"
